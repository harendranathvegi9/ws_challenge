{
  "name": "materialize-sage",
  "description": "A materialize css wordpress theme based on sage",
  "main": "",
  "authors": [
    "Anthony Delgado <me@anthonydelgado.me>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/anthonydelgado/materialize-sage",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "materialize": "^0.97.7",
    "bootstrap-sass": "3.3.6",
    "components-font-awesome": "^4.7.0"
  },
  "overrides": {
    "components-font-awesome": {
      "main": [
        "./scss/font-awesome.scss"
      ]
    },
    "bootstrap-sass": {
      "main": [
        "./assets/fonts/bootstrap/glyphicons-halflings-regular.eot",
        "./assets/fonts/bootstrap/glyphicons-halflings-regular.svg",
        "./assets/fonts/bootstrap/glyphicons-halflings-regular.ttf",
        "./assets/fonts/bootstrap/glyphicons-halflings-regular.woff",
        "./assets/fonts/bootstrap/glyphicons-halflings-regular.woff2"
      ]
    }
  },
  "resolutions": {
    "components-font-awesome": "~4.0.3"
  }
}
